<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/imgInsideDialog"
        android:layout_width="@dimen/_400sdp"
        android:layout_height="@dimen/_260sdp"
        android:src="@drawable/tujuann"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_btn_exit_dialog1"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:src="@drawable/group_18"
        app:layout_constraintBottom_toBottomOf="@+id/imgInsideDialog"
        app:layout_constraintEnd_toEndOf="@+id/imgInsideDialog" />

</androidx.constraintlayout.widget.ConstraintLayout>