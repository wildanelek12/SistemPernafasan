<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityOrgan"
    android:background="@drawable/frame_materi2">


    <ImageView
        android:id="@+id/btnBackOrgan"
        android:layout_width="@dimen/_40sdp"
        android:layout_height="@dimen/_40sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginLeft="@dimen/_5sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/btn_back"/>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/comforta_bold"
            android:text="Organ Sistem Pernafasan"
            android:textSize="@dimen/_20ssp"
            android:textColor="#2d2d2d"
            app:layout_constraintBottom_toTopOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="@dimen/_450sdp"
            android:layout_height="@dimen/_200sdp"
            android:layout_marginTop="@dimen/_50sdp"
            android:src="@drawable/organges"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnHidung"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView7" >

            <TextView
                android:id="@+id/txtHidung"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/comforta_bold"
                android:text="Hidung"

                android:textColor="#2d2d2d"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnBronkus"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintStart_toStartOf="@+id/btnHidung"
            app:layout_constraintTop_toBottomOf="@+id/btnHidung" >

            <TextView
                android:id="@+id/txtBronkus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/comforta_bold"
                android:text="Bronkus"
                android:textColor="#2d2d2d"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnBronkiolus"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintEnd_toEndOf="@+id/btnBronkus"
            app:layout_constraintStart_toStartOf="@+id/btnBronkus"
            app:layout_constraintTop_toBottomOf="@+id/btnBronkus" >

            <TextView
                android:id="@+id/txtBronkiolus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/comforta_bold"
                android:text="Bronkiolus"
                android:textColor="#2d2d2d"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnAlveolus"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintEnd_toEndOf="@+id/btnBronkiolus"
            app:layout_constraintStart_toStartOf="@+id/btnBronkiolus"
            app:layout_constraintTop_toBottomOf="@+id/btnBronkiolus" >
            <TextView
                android:id="@+id/txtAlveolus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Alveolus"
                android:fontFamily="@font/comforta_bold"
                android:textColor="#2d2d2d"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnFaring"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginRight="@dimen/_33sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintBottom_toBottomOf="@+id/btnHidung"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btnHidung" >
            <TextView
                android:id="@+id/txtFaring"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Faring"
                android:fontFamily="@font/comforta_bold"
                android:textSize="@dimen/_14ssp"
                android:textColor="#2d2d2d"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnLaring"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintEnd_toEndOf="@+id/btnFaring"
            app:layout_constraintStart_toStartOf="@+id/btnFaring"
            app:layout_constraintTop_toBottomOf="@+id/btnFaring" >

            <TextView
                android:id="@+id/txtLaring"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/comforta_bold"
                android:text="Laring"
                android:textColor="#2d2d2d"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/btnTrakea"
            android:layout_width="@dimen/_100sdp"
            android:layout_height="@dimen/_33sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintEnd_toEndOf="@+id/btnLaring"
            app:layout_constraintStart_toStartOf="@+id/btnLaring"
            app:layout_constraintTop_toBottomOf="@+id/btnLaring" >

            <TextView
                android:id="@+id/txtTrakea"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/comforta_bold"
                android:text="Trakea"
                android:textColor="#2d2d2d"
                android:textSize="@dimen/_14ssp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout

            android:id="@+id/btnParu"
            android:layout_width="@dimen/_100sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_height="@dimen/_33sdp"
            android:background="@drawable/border_organ"
            app:layout_constraintEnd_toEndOf="@+id/btnTrakea"
            app:layout_constraintStart_toStartOf="@+id/btnTrakea"
            app:layout_constraintTop_toBottomOf="@+id/btnTrakea" >
            <TextView
                android:id="@+id/txtParu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Paru Paru"
                android:fontFamily="@font/comforta_bold"
                android:textSize="@dimen/_14ssp"
                android:textColor="#2d2d2d"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>




    <ImageView
        android:id="@+id/imgBtn_sound_off"
        android:layout_width="@dimen/_40sdp"
        android:layout_height="@dimen/_40sdp"
        android:src="@drawable/sound_off"
        android:layout_marginBottom="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imgBtn_sound"
        android:layout_width="@dimen/_40sdp"
        android:layout_height="@dimen/_40sdp"
        android:src="@drawable/sound_on"
        android:layout_marginBottom="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>