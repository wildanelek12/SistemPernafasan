<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu"
    android:background="@drawable/bg2">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/_300sdp"
        android:layout_height="@dimen/_75sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:src="@drawable/home_judul"
        app:layout_constraintEnd_toStartOf="@+id/imageView10"
        app:layout_constraintStart_toEndOf="@+id/img_btn_petunjuk"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="@dimen/_300sdp"
        android:layout_height="@dimen/_200sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toEndOf="@+id/img_btn_petunjuk">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="@dimen/_400sdp"
            android:layout_height="@dimen/_200sdp"
            android:src="@drawable/laptop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/btnMenuMateri"
            android:layout_width="@dimen/_75sdp"
            android:layout_height="@dimen/_75sdp"
            android:layout_marginTop="@dimen/_30sdp"
            android:src="@drawable/home_btnmateri"
            app:layout_constraintEnd_toStartOf="@+id/btnMenuVideo"
            app:layout_constraintTop_toTopOf="@+id/btnMenuVideo" />

        <ImageView
            android:id="@+id/btnMenuVideo"
            android:layout_width="@dimen/_75sdp"
            android:layout_height="@dimen/_75sdp"
            android:layout_marginBottom="@dimen/_75sdp"
            android:src="@drawable/home_btnvideo"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/btnMenuLatihan"
            android:layout_width="@dimen/_75sdp"
            android:layout_height="@dimen/_75sdp"
            android:layout_marginTop="@dimen/_30sdp"
            android:src="@drawable/home_btnlatihan"
            app:layout_constraintStart_toEndOf="@+id/btnMenuVideo"
            app:layout_constraintTop_toTopOf="@+id/btnMenuVideo" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/boy1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/img_btn_petunjuk"
        android:layout_width="@dimen/_43sdp"
        android:layout_height="@dimen/_48sdp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/home_btnpetunjuk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/img_btn_profile"
        android:layout_width="@dimen/_43sdp"
        android:layout_height="@dimen/_48sdp"
        android:src="@drawable/home_btnprofile"
        app:layout_constraintStart_toStartOf="@+id/img_btn_petunjuk"
        app:layout_constraintTop_toBottomOf="@+id/img_btn_petunjuk"
        android:layout_marginTop="@dimen/_3sdp"/>

    <ImageView
        android:id="@+id/img_btn_info"
        android:layout_width="@dimen/_43sdp"
        android:layout_height="@dimen/_48sdp"
        android:src="@drawable/home_btntujuan"
        app:layout_constraintStart_toStartOf="@+id/img_btn_profile"
        app:layout_constraintTop_toBottomOf="@+id/img_btn_profile"
        android:layout_marginTop="@dimen/_3sdp"/>

    <ImageView
        android:id="@+id/img_btn_exit"
        android:layout_width="@dimen/_43sdp"
        android:layout_height="@dimen/_48sdp"
        android:src="@drawable/home_btnclose"
        app:layout_constraintStart_toStartOf="@+id/img_btn_info"
        app:layout_constraintTop_toBottomOf="@+id/img_btn_info"
        android:layout_marginTop="@dimen/_3sdp" />



    <ImageView
        android:id="@+id/imgBtn_sound_off"
        android:layout_width="@dimen/_40sdp"
        android:layout_height="@dimen/_40sdp"
        android:src="@drawable/sound_off"
        android:layout_marginBottom="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imgBtn_sound"
        android:layout_width="@dimen/_40sdp"
        android:layout_height="@dimen/_40sdp"
        android:src="@drawable/sound_on"
        android:layout_marginBottom="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>